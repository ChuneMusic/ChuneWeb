{
  "name": "chune",
  "version": "0.0.1",
  "description": "Chune is a personalized music news feed featuring articles from a variety of music magazines as well as artist interviews from hundreds of curated YouTube channels",
  "engines": {
    "node": "8.11.2"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack --watch",
    "lint": "eslint ./ --ignore-path .gitignore",
    "seed": "node server/seeders/seedVideos.js",
    "updateVideos": "node utils/scripts/updateLatestYoutubeData.js",
    "start-scrape": "node utils/scripts/scrape_all_artist_articles.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
    "start-dev-win": "npm run set-node-env-win & npm run build-client-watch",
    "start-heroku": "NODE_ENV='development' npm run build-client & npm run start",
    "set-node-env-win": "SET NODE_ENV='development'",
    "start-server": "nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/boilermaker-test' jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.16.1",
    "compression": "1.7.1",
    "dotenv": "6.0.0",
    "express": "4.14.1",
    "firebase": "5.0.4",
    "firebase-admin": "5.12.1",
    "firebaseui": "3.2.0",
    "geodist": "0.2.1",
    "googleapis": "27.0.0",
    "history": "4.6.3",
    "lodash": "4.17.10",
    "mixpanel-browser": "2.22.4",
    "moment": "2.21.0",
    "morgan": "1.8.1",
    "node-datetime": "2.0.6",
    "node-spotify-api": "1.0.7",
    "pg": "6.4.2",
    "promise": "8.0.1",
    "prop-types": "15.5.8",
    "react": "16.4.1",
    "react-autosuggest": "9.3.4",
    "react-dom": "16.4.1",
    "react-materialize": "1.1.2",
    "react-mixpanel": "0.0.11",
    "react-redux": "5.0.7",
    "react-redux-geolocation": "1.0.14",
    "react-responsive": "4.1.0",
    "react-router-dom": "4.1.1",
    "react-spotify-player": "1.0.4",
    "react-tap-event-plugin": "3.0.3",
    "react-tweet-embed": "1.1.1",
    "react-waypoint": "8.0.2",
    "react-youtube": "7.5.0",
    "redux": "3.6.0",
    "redux-logger": "2.8.1",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "request": "2.85.0",
    "reselect": "3.0.1",
    "scrape-it": "5.0.5",
    "sequelize": "4.38.0",
    "sequelize-cli": "4.0.0",
    "socket.io": "2.0.3",
    "video-react": "0.13.0",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "3.0.1",
    "@material-ui/icons": "3.0.1",
    "babel-core": "6.22.1",
    "babel-jest": "23.4.2",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "css-loader": "0.28.11",
    "enzyme": "^3.6.0",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "redux-devtools-extension": "2.13.2",
    "style-loader": "0.23.0",
    "webpack": "2.7.0",
    "webpack-livereload-plugin": "1.0.0"
  },
  "babel": {
    "presets": [
      ["env"],
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-class-properties"
    ]
  }
}
